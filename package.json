  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development",
    "start:fr": "ng serve --configuration=development,fr",
    "start:en": "ng serve --configuration=development,en",
    "build": "yarn build:prod",
    "build:prod": "ng build --prod",
    "build:dev": "yarn build --optimization=false --sourceMap=true --no-buildOptimizer --statsJson",
    "test": "yarn test:lint && yarn test:unit && yarn test:e2e",
    "test:unit": "ng test",
    "test:lint": "ng lint",
    "test:e2e": "ng e2e"
  },
